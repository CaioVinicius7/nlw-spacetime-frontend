{
  "extends": ["next/core-web-vitals", "@rocketseat/eslint-config/react"],
  "plugins": ["prettier"],
  "rules": {
    "@next/next/no-img-element": "off",
    "no-useless-constructor": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "arrowParens": "always",
        "semi": true,
        "endOfLine": "auto",
        "trailingComma": "none"
      }
    ]
  }
}
